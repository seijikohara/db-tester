<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<!--
  Checkstyle suppressions for db-tester project.
  Use sparingly and document reasons for each suppression.
-->
<suppressions>
  <!--
    Allow star imports for JUnit assertions in test code.
    Static imports for assertions improve test readability.
  -->
  <suppress checks="AvoidStarImport" files=".*Test\.java$"/>
</suppressions>
